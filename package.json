{
  "name": "unusualwhales-mcp",
  "version": "0.1.3",
  "description": "MCP server for Unusual Whales financial data and analytics",
  "type": "module",
  "bin": {
    "unusualwhales-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "unusualwhales-mcp",
    "unusualwhales",
    "mcp",
    "financial-data",
    "model-context-protocol",
    "options-flow",
    "claude",
    "claude-desktop",
    "financial-api",
    "market-data",
    "ai-finance",
    "anthropic",
    "trading-data",
    "options-analytics",
    "unusual-activity",
    "financial-tools",
    "market-analytics",
    "trading-intelligence"
  ],
  "author": "niedasen",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "yargs": "^17.7.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/yargs": "^17.0.32",
    "typescript": "^5.3.3"
  }
}
